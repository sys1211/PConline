<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tf.pconline">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:screenOrientation="portrait"
        android:configChanges="orientation"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.SearchActivity"
            android:launchMode="singleTop"
            ></activity>
        <activity android:name=".Activitys.EnterActivity"/>
        <activity android:name=".Activitys.DengLu_Activity"/>
        <activity android:name=".Activitys.AboutUs"/>
        <activity android:name=".Activitys.Fonts"/>
        <activity android:name=".Activitys.Wifi"/>
        <activity android:name=".Activitys.Line"/>
        <activity android:name=".Activitys.Share"/>
        <activity android:name=".Activitys.Collect"/>
    </application>

</manifest>